    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "prisma": "^5.6.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
